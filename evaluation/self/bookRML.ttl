@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://www.example.com/books/> .

ex:BookMap
  rr:logicalTable [
    rr:tableName "catalog"
  ];

  rr:subjectMap [
    rr:template "http://example.com/book/{@id}";
    rr:class ex:book
  ];

  rr:predicateObjectMap [
    rr:predicate ex:author;
    rr:objectMap [
      rml:reference "author";
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate ex:title;
    rr:objectMap [
      rml:reference "title";
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate ex:genre;
    rr:objectMap [
      rml:reference "genre";
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate ex:price;
    rr:objectMap [
      rml:reference "price";
      rr:datatype xsd:decimal;
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate ex:publish_date;
    rr:objectMap [
      rml:reference "publish_date";
      rr:datatype xsd:date;
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate ex:description;
    rr:objectMap [
      rml:reference "description";
    ]
  ].
